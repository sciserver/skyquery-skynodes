<metadata>
  <table name="[dbo].[PhotoObj]">
    <column name="[objID]">
      <summary> Numerical ID. </summary>
</column>
    <column name="[csID]">
      <summary> Catalina Surveys identification (coded from the star's RA_2000 and DEC_2000 in the format: CSS_Jhhmmss.s+ddmmss). </summary>
</column>
    <column name="[ra]">
      <summary> Right ascension in decimal degrees (J2000). </summary>
<unit>deg J2000</unit>
</column>
    <column name="[dec]">
      <summary> Declination in decimal degrees (J2000). </summary>
<unit>deg J2000</unit>
</column>
    <column name="[cx]">
      <summary> Cartesian X (J2000)</summary>
</column>
    <column name="[cy]">
      <summary> Cartesian Y (J2000)</summary>
</column>
    <column name="[cz]">
      <summary> Cartesian Z (J2000)</summary>
</column>
    <column name="[htmid]">
      <summary> HTM ID (J2000)</summary>
</column>
    <column name="[zoneid]">
      <summary> Zone ID </summary>
</column>
    <column name="[V]">
      <summary> Magnitude in V band. </summary>
<unit> mag </unit>
</column>
    <column name="[Period]">
      <summary>  Period in days. </summary>
<unit> days </unit>
</column>
    <column name="[Amplitude]">
      <summary>  Amplitude of the variation. </summary>
</column>
    <column name="[NumberObs]">
      <summary> Number of observations. </summary>
</column>
    <column name="[Type]">
      <summary> Variable type. </summary>
</column>
  </table>
</metadata>